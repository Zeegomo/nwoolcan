<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.text.Text?>
<VBox xmlns="http://javafx.com/javafx/8.0.172-ea"
      xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="nwoolcan.view.review.EvaluationView">
    <FlowPane fx:id="scoreFlowPane">
        <Label text="Score: "/>
        <TextField fx:id="scoreTextField"/>
        <Label fx:id="scoreLabel"/>
        <Label text="/"/>
        <Label fx:id="maxScore"/>
    </FlowPane>
    <FlowPane fx:id="notesFlowPane">
        <Label text="Notes: "/>
        <TextArea fx:id="notesTextArea"/>
        <Text fx:id="notesText"/>
    </FlowPane>
</VBox>