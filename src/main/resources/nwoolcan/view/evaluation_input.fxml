<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox
        xmlns="http://javafx.com/javafx/8.0.172-ea"
        xmlns:fx="http://javafx.com/fxml/1"
        fx:controller="nwoolcan.view.review.EvaluationInputController">
    <FlowPane>
        <Label text="Score: "/>
        <TextField fx:id="score"/>
        <Label text="/"/>
        <Label fx:id="maxScore"/>
    </FlowPane>
    <FlowPane>
        <Label text="Notes: "/>
        <TextArea fx:id="notes"/>
    </FlowPane>
</VBox>